{
    "name": "Web Access",
    "version": "1.0.0",
    "description": "Web accessibility extensions",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "match_about_blank": true,
            "run_at": "document_start",
            "js": [
                "content.js",
                "runner.js",
                "htmlcs.js",
                "axe.js"
            ]
        }
    ]
}